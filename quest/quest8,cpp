#include <iostream>
#include<string>
#include<vector>
using namespace std;
void number(string  a,string c, double &val, double &val1){
	vector <string>num; num.push_back("0");num.push_back("1");num.push_back("2");num.push_back("3");num.push_back("4");num.push_back("5");num.push_back("6");num.push_back("7");num.push_back("8");num.push_back("9");
	vector <string>let; let.push_back("cero");let.push_back("uno");let.push_back("dos");let.push_back("tres");let.push_back("cuatro");let.push_back("cinco");let.push_back("seis");let.push_back("siete");let.push_back("ocho");let.push_back("nueve");
	for(int i=0;i<10;i++){
		if((num[i]==a)||(let[i]==a))
			val=i;

		if((num[i]==c)||(let[i]==c))
				val1=i;
}}
